            <Label fx:id="usernameLabel" layoutX="319.0" layoutY="197.0" prefHeight="17.0" prefWidth="76.0" />
            <ImageView fx:id="profileImageView" fitHeight="164.0" fitWidth="154.0" layoutX="298.0" layoutY="50.0" preserveRatio="true" />
