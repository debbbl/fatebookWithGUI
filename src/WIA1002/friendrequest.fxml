      <children>
           <Button fx:id="rejectButton" layoutX="87.0" layoutY="389.0" onAction="#rejectButtonOnAction" prefHeight="25.0" prefWidth="59.0" style="-fx-background-color: #ADD8E6;" text="Reject" textFill="WHITE">
