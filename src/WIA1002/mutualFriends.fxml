<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="300" prefWidth="400" spacing="10" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WIA1002.mutualFriendsController">

    <Label style="-fx-font-size: 20px;" text="Find Mutual Friends" />

    <ListView fx:id="mutualFriendsListView" prefHeight="150" prefWidth="200" />

    <Label fx:id="resultLabel" />
    <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" onAction="#cancelButtonOnAction" />

    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>

</VBox>
